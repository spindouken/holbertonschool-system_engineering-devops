#!/usr/bin/env bash
# does file school exist? if yes, display
file=school
if [ -e $file ]
then
	echo "$file exists"
	if [ -s $file ]
	then
		echo "$file file is not empty"
	else
		echo "$file file is empty"
	fi
	if [ -f $file ]
	then
		echo "$file file is a regular file"
	fi
else
	echo "$file file does not exist"
fi
